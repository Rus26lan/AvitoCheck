<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.rundgrun.ui.primorskiy.PrimorskiyFragment"
        android:label="@string/primorskiy"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.rundgrun.ui.pionerskiy.DashboardFragment"
        android:label="@string/pionerskiy"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.rundgrun.ui.frunsenskiy.NotificationsFragment"
        android:label="@string/frunsenskiy"
        tools:layout="@layout/fragment_notifications" />
</navigation>